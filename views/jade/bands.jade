    body
      block content
      script(src='https://ajax.googleapis.com/ajax/libs/jquery/2.0.3/jquery.min.js')
      script(src='/javascripts/global.js')
      script(src='/javascripts/date.js')

    #wrapper
      header
        #header-text
          h1#logo SXDiscover
          p#tagline These bands from your Spotify playlists are playing SXSW.
      article
        each key in json
          .date
            h2 !{key[0].date}
          each item in key[0].bands
            .band-container
              h2.artist !{item[0]}
              h3.venue !{item[1]}. !{item[2]}
